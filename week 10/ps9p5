total = 0
tax = 0

def compute_total_and_tax(qty, unit_price):
    global total, tax
    total = qty * unit_price
    tax = total * 0.07
qty = int(input("Enter quantity of the item: "))
unit_price = float(input("Enter unit price: "))

compute_total_and_tax(qty, unit_price)

print("Total:", total)
print("Tax:", tax)
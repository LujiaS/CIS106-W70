
quantity = int(input("Enter quantity: "))

if quantity >= 1000:
    unitp = 3
else:
    unitp = 5

extended_price = quantity * unitp

tax = extended_price * 0.07

total = extended_price + tax

print("Quantity", quantity)
print("Unit Price", unitp)
print("Extended Price", extended_price)
print("Tax", tax)
print("Total", total)

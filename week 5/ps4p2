partno = int(input('enter part number'))
qty = input('enter quantity')
if partno == 10 or partno == 55:
    unitp = 1.00
elif partno == 90:
    unitp = 2.00
elif partno == 80 or partno == 70:
    unitp = 3.00
else:
    unitp = 5.00
total = float(qty) * unitp

print('Partner number', partno)
print('Unit price', unitp)
print('Total', total)